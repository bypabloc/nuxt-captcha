{
  "name": "nuxt-captcha",
  "private": true,
  "type": "module",
  "scripts": {
    "icons": "echo 'SVG icons generated successfully'",
    "generate:icons": "node scripts/generate-svg-icons.js && pnpm icons",
    "build": "nuxt build",
    "dev": "pnpm generate:icons && nuxt dev",
    "generate:ares": "nuxt generate --dotenv .env.ares",
    "generate:pandora": "nuxt generate --dotenv .env.pandora",
    "generate:production": "nuxt generate --dotenv .env.production",
    "generate": "rm -rf .output && pnpm generate:icons && nuxt generate",
    "generate:debug": "rm -rf .output && pnpm nuxt:clean-cache && NUXT_LOG_LEVEL=verbose DEBUG=nuxt:* nuxt generate > generate-log.txt 2>&1",
    "postinstall": "pnpm generate:icons && nuxt prepare",
    "lint": "eslint --ext .ts,.js,.vue",
    "lint:fix": "eslint --fix --ext .ts,.js,.vue",
    "format": "prettier --write \"**/*.{ts,js,vue,css,scss}\" && eslint --fix --ext .ts,.js,.vue",
    "preview": "nuxt preview",
    "reset-vscode": "rm -rf .vscode/.eslintcache && rm -rf ~/.vscode/extensions/esbenp.prettier-vscode-*/node_modules/.cache",
    "reinstall": "rm -rf node_modules pnpm-lock.yaml .nuxt .output && pnpm store prune && pnpm install",
    "nuxt:clean-cache": "rm -rf .nuxt .output",
    "nuxt:prepare": "nuxt prepare",
    "coverage": "pnpm generate:icons && vitest run --coverage",
    "check": "pnpm lint && pnpm typecheck && pnpm coverage && rm -rf .output && pnpm generate",
    "run-server": "http-server .output/public -p 3000 -o -s",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/eslint": "1.3.0",
    "@nuxt/fonts": "0.11.2",
    "@nuxt/icon": "1.12.0",
    "@nuxt/image": "1.10.0",
    "@nuxt/schema": "^3.17.2",
    "@nuxt/scripts": "0.11.6",
    "@nuxtjs/i18n": "^9.5.4",
    "@pinia/nuxt": "^0.11.0",
    "@unhead/vue": "^2.0.8",
    "@unocss/nuxt": "66.1.1",
    "@unocss/preset-attributify": "66.1.1",
    "@unocss/preset-icons": "66.1.1",
    "@unocss/preset-mini": "66.1.1",
    "@unocss/preset-uno": "66.1.1",
    "@unocss/preset-web-fonts": "66.1.1",
    "@vee-validate/zod": "^4.15.0",
    "@vueuse/core": "^13.1.0",
    "@vueuse/integrations": "^13.1.0",
    "focus-trap": "^7.6.4",
    "nuxt": "^3.17.2",
    "pinia": "^3.0.2",
    "unocss": "66.1.1",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@iconify-json/ci": "^1.2.2",
    "@iconify-json/garden": "^1.2.1",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/svg-spinners": "^1.2.2",
    "@iconify/types": "^2.0.0",
    "@nuxt/test-utils": "3.18.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/node": "^22.15.17",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@unocss/preset-wind4": "66.1.1",
    "@vitest/coverage-istanbul": "^3.1.3",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.1.0",
    "happy-dom": "^17.4.6",
    "jsdom": "^26.1.0",
    "playwright-core": "^1.52.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prettier-plugin-vue": "^1.1.6",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  },
  "resolutions": {
    "eslint": "^9.24.0"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
